# Set the numlock to be on. -> Because it makes sense...
numlockx &

# Bind keys. 
xmodmap ~/.config/.xmodmap &

# Monitors. 
bash ./.config/autostart/monitorsetup.sh

# Wallpaper restore
nitrogen --restore

# Start picom
# picom
picom -b 

# jetbrains-toolbox

# Home Bin
export PATH=$PATH:/home/caleb/bin

[[ -f ~/.profile ]] && . ~/.profile
